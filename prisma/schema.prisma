
generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Customer {
    id Int  @id @default(autoincrement())
    first_name String
    last_name String
    email String @unique
    password String
    tool_assemblies Tool_assembly[]
}

model Dictonary {
  id Int @id @default(autoincrement())
  property_name String
  full_property_name String?
  unit String?
  unit_explanation String?
}

model Tool_assembly {
    id Int @id @default(autoincrement())
    name String?
    customer Customer? @relation(fields: [customer_id], references: [id])
    customer_id Int?
    used_cutting_item Tool_assembly_cutting_item[]
    used_adaptive_item Tool_assembly_adaptive_item[]
    used_tool_item Tool_assembly_tool_item[]
    used_assembly_item Tool_assembly_assembly_item[]
    created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

model Assembly_item_category {
id     Int       @id @default(autoincrement())
name  String
img String
assembly_items Assembly_item[]
 created_at DateTime @default(now())
updated_at DateTime @updatedAt
}

model Assembly_item {
  id    Int       @id @default(autoincrement())
  name  String
  connecting_adaptive_item Adaptive_assembly[]
  connecting_tool_item Assembly_tool[]
  used_for_tool_assembly Tool_assembly_assembly_item[]
  category Assembly_item_category? @relation(fields: [category_id], references: [id])
  category_id Int?
  img   String?
  adjby Int?
  dcbx  String?
  dcbn  String?
  ccms  String?
  ccws  String?
  dinn  Float?
  diout Float?
